package com.tu.lunchy.dao.objects;

public class Menu {
	
	private int menuId;
	private int mealId;
	private String menuName;
	
	public Menu() {
	}
	
	public Menu(int mealId, String menuName) {
		this.mealId = mealId;
		this.menuName = menuName;
	}
	
	public Menu(int menuId, int mealId, String menuName) {
		this.menuId = menuId;
		this.mealId = mealId;
		this.menuName = menuName;
	}
	
	public int getMenuId() {
		return menuId;
	}
	public void setMenuId(int menuId) {
		this.menuId = menuId;
	}
	public int getMealId() {
		return mealId;
	}
	public void setMealId(int mealId) {
		this.mealId = mealId;
	}
	public String getMenuName() {
		return menuName;
	}
	public void setMenuName(String menuName) {
		this.menuName = menuName;
	}

	
}
